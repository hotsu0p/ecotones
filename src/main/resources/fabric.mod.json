{
  "schemaVersion": 1,
  "id": "ecotones",
  "version": "${version}",

  "name": "Ecotones",
  "description": "A biome overhaul mod with realistic and expansive biomes filled with life",
  "authors": [
    "jaskarth"
  ],
  "contact": {
    "sources": "https://github.com/jaskarth/ecotones"
  },

  "license": "LGPLv3",
  "icon": "assets/ecotones/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.jaskarth.ecotones.Ecotones"
    ],
    "client": [
      "com.jaskarth.ecotones.EcotonesClient"
    ],
    "emi": [
      "com.jaskarth.ecotones.util.compat.emi.EcotonesEmi"
    ]
  },
  "mixins": [
    "ecotones.mixins.json"
  ],

  "accessWidener" : "ecotones.aw",

  "depends": {
    "fabricloader": ">=0.7.2",
    "fabric": "*",
    "minecraft": ">=1.20-"
  },
  "suggests": {
    "traverse": "*",
    "terrestria": "*"
  }
}
